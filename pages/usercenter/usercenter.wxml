<!-- pages/usercenter/usercenter.wxml -->
<view>
  <!-- 如果只是展示用户头像昵称，可以使用 <open-data /> 组件 -->
  <view class="userinfo">
    <view class="userinfo-avatar">
      <open-data type="userAvatarUrl"></open-data>
    </view>
    <open-data type="userNickName"></open-data>
  </view>
  <view class="weui-cells__title">个人设置</view>
   <view class="weui-cells weui-cells_after-title">
            <navigator url="../mycollection/mycollection" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd">我的收藏</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
            <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd">我的上传</view>
                 <view class="weui-cell__ft">暂不可用</view>
            </navigator>
        </view>
  <text class='text-bottom'>更多功能开发中， qq群：962538625</text>
</view>